TestCase subclass: #TestDestino	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Tests'!!TestDestino methodsFor: 'tests' stamp: 'ds 10/11/2021 11:29'!testUnDestinoNiTerminalNiHospitalTieneUnValorDefinidoPorElChofer	|unChofer obras|	obras := Destino destino: 'Obras' distancia: 5 peajes: 5.	unChofer := ChoferComun nombre: 'Juan' cobra: 150.		self assert: (obras costoPeajeCon: unChofer ) closeTo: 500. ! !TestCase subclass: #TestValorPeajeEscalonado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Tests'!!TestValorPeajeEscalonado methodsFor: 'tests' stamp: 'ds 10/11/2021 11:07'!testConUnPeajeElValorDelPeajeEsElStandard	|valorPeaje unChofer |	unChofer := ChoferComun nombre: 'Luis' cobra: 100. 	valorPeaje := ValorPeajeEscalonado por: unChofer.		self assert: (valorPeaje valorPara: 1) closeTo: 100.! !